<Window x:Class="CTKS_Chart.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:liveChart="clr-namespace:LiveChart;assembly=LiveChart"
        xmlns:local="clr-namespace:CTKS_Chart"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="MainWindow"
        Width="1200"
        Height="850"
        mc:Ignorable="d">
    <Grid>
        <Canvas x:Name="main_canvas"
                Background="Black"
                SizeChanged="main_canvas_SizeChanged"
                MouseMove="Canvas_MouseMove"
                Loaded="main_canvas_Loaded">
            <TextBlock x:Name="coordinates"
                       Foreground="White"
                       Panel.ZIndex="100"
                       Text="Ahoj" />
        </Canvas>
    </Grid>
</Window>
